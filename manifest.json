{
    "name": "iPARQ Validator",
    "description": "A chrome extension to validate iPARQ Parking Permits in accordance to Google Sheets",
    "version": "0.0.1",
    "manifest_version": 2,
    "background": {
      "scripts": ["background.js"]
    },
    "content_scripts": [{
      "js": ["jquery-3.6.0.min.js", "popup.js"],
      "matches": ["http://*/*", "https://*/*"]
    }],
    "permissions": ["storage"],
    "browser_action": {
      "default_title": "Popup",
      "default_popup":"popup.html"
    }
  }